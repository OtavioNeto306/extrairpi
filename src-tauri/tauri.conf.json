{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Extrator RPI",
  "version": "0.1.0",
  "identifier": "com.extrairrpi.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Extrator de RPI",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "Extrator RPI",
    "category": "Productivity",
    "shortDescription": "Extrator de deferimentos de RPI de arquivos PDF",
    "longDescription": "Aplicativo para extrair e filtrar deferimentos de RPI de arquivos PDF, removendo registros sem procurador."
  }
}
